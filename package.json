{
	"name": "receiver-bot",
	"version": "0.0.1",
	"description": "An example receiver bot written using @discordjs/voice",
	"main": "dist/index.js",
	"scripts": {
		"whisper": "ts-node-dev --inspect -- tests/whisper.ts",
		"start": "ts-node-dev --inspect -- src/index.ts",
		"start:build": "npm run build && node dist",
		"lint": "prettier --cache --check . && eslint src --ext mjs,js,ts --cache",
		"format": "prettier --cache --write . && eslint src --ext mjs,js,ts --fix --cache",
		"build": "rimraf dist && tsc",
		"build:check": "tsc --noEmit --incremental false",
		"test": "jest"
	},
	"author": "Amish Shah <contact@shah.gg>",
	"license": "MIT",
	"dependencies": {
		"@discordjs/opus": "^0.9.0",
		"@discordjs/voice": "^0.16.0",
		"discord.js": "^14.11.0",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"fluent-ffmpeg": "^2.1.2",
		"libsodium-wrappers": "^0.7.10",
		"node-crc": "^3.0.1",
		"openai": "^3.2.1",
		"prism-media": "^2.0.0-alpha.0",
		"socket.io": "^4.6.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/fluent-ffmpeg": "^2.1.21",
		"@types/jest": "^29.5.1",
		"@typescript-eslint/eslint-plugin": "^5.36.1",
		"@typescript-eslint/parser": "^5.36.1",
		"eslint": "^8.23.0",
		"eslint-config-marine": "^9.4.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.5.0",
		"eslint-plugin-import": "^2.26.0",
		"jest": "^29.5.0",
		"prettier": "^2.7.1",
		"rimraf": "^5.0.1",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.0",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.1.3"
	}
}
